permute gi_status beta = _b[gi_status] se = _se[gi_status] df = e(df_r), ///
  reps(500) rseed(123) saving("placebo_GI.dta"): ///
  reghdfe hdi gi_status, absorb(id year) vce(cluster id)

use "placebo_GI.dta", clear
  
#delimit ;
dpplot beta, 
  xline(0.2140569, lc(black*0.5) lp(dash))   /// replace with your actual coefficient
  xline(0, lc(black) lp(solid))             
  xlabel(0(0.1)0.4)                         /// adjust range depending on your coefficients
  xtitle(`"{fontface "Times New Roman":Random sampling coefficients}"') xlabel(, format(%4.2f) labsize(small))
  ytitle(`"{fontface "Times New Roman":Kernel density}"') ylabel(, nogrid format(%4.2f) labsize(small)) 
  note("") caption("") graphregion(fcolor(white)) msymbol(smcircle_hollow) mcolor(black) ;
#delimit cr

graph export "placebo_GI.png", width(1000) replace
restore
